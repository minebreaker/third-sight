{
  "name": "third-sight",
  "version": "0.2.0",
  "description": "",
  "homepage": "https://github.com/minebreaker/third-sight",
  "bugs": {
    "url": "https://github.com/minebreaker/third-sight/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minebreaker/third-sight.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run assemble && npm run lint && npm run test && npm run web-ext-build",
    "build:clean": "npm run clean && npm run build",
    "assemble": "npm run compile && npm run bundle && npm run remover && npm run copy",
    "compile": "tsc -p tsconfig.json",
    "bundle": "webpack",
    "copy": "npm run copy-index && npm run copy-manifest && npm run copy-icon",
    "copy-index": "cpx static/index.html ./build/out",
    "copy-manifest": "cpx static/manifest.json ./build/out",
    "copy-icon": "cpx static/icon.svg ./build/out",
    "web-ext-run": "cd ./build/out && web-ext run",
    "web-ext-build": "cd ./build/out && web-ext build --overwrite-dest",
    "web-ext-lint": "cd ./build/out && web-ext lint",
    "remover": "node ./script/remover.js",
    "version": "node ./script/bump-manifest.js %npm_package_version% && git add ./static/manifest.json",
    "test": "mocha --config .mocharc.js",
    "clean": "rimraf ./build/*",
    "lint": "npm run lint-eslint && npm run web-ext-lint",
    "lint-eslint": "eslint src",
    "watchCompile": "tsc -p tsconfig.json --incremental -w",
    "watchBundle": "webpack -w"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "immutable": "^4.0.0-rc.15",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@types/chai": "^4.2.22",
    "@types/lodash": "^4.14.173",
    "@types/luxon": "^2.0.4",
    "@types/mocha": "^9.0.0",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.22",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "cpx": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "web-ext": "^6.4.0",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  }
}
