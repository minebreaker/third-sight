{
  "name": "third-sight",
  "version": "0.1.0",
  "description": "",
  "homepage": "https://github.com/minebreaker/third-sight",
  "bugs": {
    "url": "https://github.com/minebreaker/third-sight/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minebreaker/third-sight.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "set NODE_ENV=production && npm run assemble & npm run lint && npm run test && npm run web-ext-build",
    "build:clean": "npm run clean && npm run build",
    "assemble": "npm run compile && npm run bundle && npm run copy",
    "compile": "tsc -p tsconfig.json",
    "bundle": "webpack",
    "copy": "npm run copy-index && npm run copy-manifest && npm run copy-icon",
    "copy-index": "cpx static/index.html ./build/out",
    "copy-manifest": "cpx static/manifest.json ./build/out",
    "copy-icon": "cpx static/icon.svg ./build/out",
    "web-ext-run": "cd ./build/out && web-ext run",
    "web-ext-build": "cd ./build/out && web-ext build",
    "test": "mocha --config .mocharc.js",
    "clean": "rimraf ./build/*",
    "lint": "npm run lint-web-ext",
    "lint-web-ext": "cd ./build/out && web-ext lint",
    "watchCompile": "tsc -p tsconfig.json --incremental -w",
    "watchBundle": "webpack -w"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@types/chai": "^4.2.18",
    "@types/lodash": "^4.14.169",
    "@types/luxon": "^1.26.5",
    "@types/mocha": "^8.2.2",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-test-renderer": "^17.0.1",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "cpx": "^1.5.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "web-ext": "^6.1.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3"
  }
}
